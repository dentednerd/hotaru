{
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "max-len": 0,
    "semi": ["error", "always"]
  },
  "extends": [
    "airbnb",
    "react-app",
    "prettier"
  ]
}
